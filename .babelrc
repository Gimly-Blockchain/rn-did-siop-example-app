{
  "presets": [
    "module:metro-react-native-babel-preset"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "extensions": [
          ".ios.ts",
          ".android.ts",
          ".ts",
          ".ios.tsx",
          ".android.tsx",
          ".jsx",
          ".js",
          ".json"
        ],
        "alias": {
          "jose/jwk/parse": "jose/dist/node/cjs/jwk/parse.js",
          "jose/jwt/verify": "jose/dist/node/cjs/jwt/verify.js"
        }
      }
    ]
  ]
}